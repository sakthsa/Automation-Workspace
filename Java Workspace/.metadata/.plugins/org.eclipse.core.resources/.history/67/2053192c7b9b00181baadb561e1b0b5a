package com.ss.test;

public class ForLoop {

	public static void main(String[] args) {
		int count = 0;
		int arr[] = {1,1,2,3,4};
		for(int i : arr) {
			count++;
		}
		System.out.println(count);
		
		aa:
			for(int i = 0; i <=3; i++) {
				bb:
				for(int j = 0; j<=3; j++) {
					if(i==2&j==2) {
					continue aa;	
					}
					System.out.print(""+i+j+" ");
				}
		System.out.println();
		
			}
		
	}

}
